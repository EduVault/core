  "license": "ISC",
  "bugs": {
    "url": "https://github.com/EduVault/server/issues"
  },
  "homepage": "https://github.com/EduVault/server#readme",
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
