{
  "name": "@eduvault/server",
  "version": "1.0.0",
  "description": "The server includes the backend api, the main app, and the homepage",
  "main": "index.js",
  "scripts": {
    "test:api": "cd ./api && yarn test",
    "test:app": "cd ./app && yarn test",
    "dev:api": "cd ./api && yarn dev",
    "dev:app": "cd ./app && yarn dev",
    "dev": "run-p dev:*",
    "build:api": "cd ./api && yarn build",
    "build:app": "cd ./app && yarn build",
    "start:api": "cd ./api && yarn start",
    "start": "run-s build:app start:api"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EduVault/server.git"
  },
  "keywords": [],
